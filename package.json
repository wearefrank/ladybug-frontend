{
  "name": "@wearefrank/ladybug",
  "version": "${project.version}",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/proxy.conf.json",
    "startCicd": "ng serve --proxy-config src/proxy.cicd.conf.json --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test 'ng serve' 4200 cypress:run",
    "e2e-interactive": "start-server-and-test 'ng serve' 4200 cypress:open",
    "e2e-interactive-webjar": "cypress open --config baseUrl=http://localhost/ladybug/",
    "e2e-webjar": "cypress run --config baseUrl=http://localhost/ladybug/",
    "lint": "ng lint",
    "prepare": "husky install",
    "installFrankRunnerEnv": "cd ../frank-runner && ./env.sh > installFrankRunnerEnv.log",
    "startServer": "cd ../frank-runner/specials/ladybug && ./restart.sh -Dupdate.strategy=none > serverStartLog.log"
  },
  "dependencies": {
    "@angular/animations": "17.0.8",
    "@angular/cdk": "17.0.4",
    "@angular/common": "17.0.8",
    "@angular/compiler": "17.0.8",
    "@angular/core": "17.0.8",
    "@angular/forms": "17.0.8",
    "@angular/localize": "17.0.8",
    "@angular/material": "17.0.4",
    "@angular/platform-browser": "17.0.8",
    "@angular/platform-browser-dynamic": "17.0.8",
    "@angular/router": "17.0.8",
    "@materia-ui/ngx-monaco-editor": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "16.0.0",
    "@popperjs/core": "^2.11.8",
    "@prettier/plugin-xml": "^3.2.2",
    "ace-builds": "^1.14.0",
    "angular-split": "^14.1.0",
    "bootstrap": "4",
    "bootstrap-icons": "^1.11.3",
    "bower": "1.8.12",
    "chai-colors": "^1.0.1",
    "diff-match-patch": "1.0.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.7.1",
    "jqwidgets-ng": "^14.1.17",
    "monaco-editor": "0.44.0",
    "ng-simple-file-tree": "^0.1.13",
    "ngx-monaco-editor-v2": "17.0.1",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "prettier": "^3.2.4",
    "rxjs": "7.4.0",
    "tslib": "2.3.1",
    "xml-beautifier": "0.5.0",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.9",
    "@angular-eslint/builder": "17.2.0",
    "@angular-eslint/eslint-plugin": "17.2.0",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "17.2.0",
    "@angular-eslint/template-parser": "17.2.0",
    "@angular/cli": "17.0.9",
    "@angular/compiler-cli": "17.0.8",
    "@types/bootstrap": "^4",
    "@types/diff-match-patch": "1.0.32",
    "@types/jasmine": "3.9.1",
    "@types/node": "16.10.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "chai-match": "^1.1.1",
    "cypress": "13.13.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.53.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "husky": "^7.0.4",
    "jasmine-core": "3.9.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "5.2.2"
